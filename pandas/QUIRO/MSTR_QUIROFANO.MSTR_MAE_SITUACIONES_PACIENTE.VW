CREATE OR REPLACE FORCE VIEW MSTR_QUIROFANO.MSTR_MAE_SITUACIONES_PACIENTE
(SITPAC_CODIGO, SITPAC_CORTO, SITPAC_NOMBRE, SITPAC_OBS, SITPAC_SITUACION)
AS 
SELECT SITPAC_CODIGO,
       SITPAC_CORTO,
       SITPAC_NOMBRE,
       SITPAC_OBS,
       SITPAC_SITUACION
  FROM REP_PRO_AGD.SITUACIONES_PACIENTES@DAE
/
