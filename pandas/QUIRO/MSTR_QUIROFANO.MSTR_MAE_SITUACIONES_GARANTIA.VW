CREATE OR REPLACE FORCE VIEW MSTR_QUIROFANO.MSTR_MAE_SITUACIONES_GARANTIA
(SITGAR_CODIGO, SITGAR_CORTO, SITGAR_NOMBRE, SITGAR_SITUACION, TIPSG_CODIGO)
AS 
SELECT SITGAR_CODIGO,
       SITGAR_CORTO,
       SITGAR_NOMBRE,
       SITGAR_SITUACION,
       TIPSG_CODIGO
  FROM REP_PRO_AGD.SITUACIONES_GARANTIA@DAE
/
