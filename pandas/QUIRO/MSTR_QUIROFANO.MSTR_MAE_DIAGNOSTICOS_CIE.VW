CREATE OR REPLACE FORCE VIEW MSTR_QUIROFANO.MSTR_MAE_DIAGNOSTICOS_CIE
(NATID_DIAGNOSTICO_CIE, NATID_GRUPO_DIAGNOSTICO_CIE, DESCR_DIAGNOSTICO_CIE, IND_HABILITADO)
AS 
SELECT CODIGO,
          NVL (SUBSTR (CODIGO, 1, INSTR (CODIGO, '.') - 1), CODIGO),
          DESCRIPCION,
          IND_HABILITADO
     FROM SIDCA_OWN.HCE_MA_CIE9@DAE
/
