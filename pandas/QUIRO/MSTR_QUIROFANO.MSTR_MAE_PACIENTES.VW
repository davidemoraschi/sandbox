CREATE OR REPLACE FORCE VIEW MSTR_QUIROFANO.MSTR_MAE_PACIENTES
(NATID_PACIENTE, NUHSA_PACIENTE, NOMBR_PACIENTE, APELL_PACIENTE, FECHA_NACIM_PACIENTE, 
 DOCUM_PACIENTE)
AS 
SELECT ID_USUARIO,
          NUHSA,
          NOMBRE,
          APELLIDO1 || ' ' || APELLIDO2 APELLIDOS,
          FECHA_NACIMIENTO,
          DOCUMENTO
     FROM HIS_OWN.COM_USUARIO@DAE
/
