CREATE OR REPLACE FORCE VIEW MSTR_SIGLO.MSTR_MAE_CATALOGO_N9
(NATID_CATALOGO_N9, DESCR_CATALOGO_N9, LDESC_CATALOGO_N9, NATID_CATALOGO_N8)
AS 
SELECT ID NATID_CATALOGO_N9,
          DESCRIPCION DESCR_CATALOGO_N9,
          CODIGOCONCATENADO LDESC_CATALOGO_N9,
          VALORPADRE NATID_CATALOGO_N8
     FROM REP_PRO_SIGLO.CAT_NIVELVALOR@SYG
    WHERE CLASIFICACION = 5 AND VALORPADRE IN (SELECT NATID_CATALOGO_N8 FROM MSTR_MAE_CATALOGO_N8)
/
