CREATE OR REPLACE FORCE VIEW MSTR_SIGLO.MSTR_MAE_CATALOGO_N0
(NATID_CATALOGO_N0, DESCR_CATALOGO_N0)
AS 
SELECT ID NATID_CATALOGO_N0, DESCRIPCION DESCR_CATALOGO_N0
     --CODIGO CODE1_CAPITULO_N0--,
     --NIVEL NIVEL,
     --VALORPADRE                                                                                                           --,
     FROM REP_PRO_SIGLO.CAT_NIVELVALOR@SYG
    WHERE CLASIFICACION = 5 AND VALORPADRE IS NULL
/
