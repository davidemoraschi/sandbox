CREATE OR REPLACE FORCE VIEW MSTR_SIGLO.MSTR_MAE_CATALOGO_N7
(NATID_CATALOGO_N7, DESCR_CATALOGO_N7, LDESC_CATALOGO_N7, NATID_CATALOGO_N6)
AS 
SELECT ID NATID_CATALOGO_N7,
          DESCRIPCION DESCR_CATALOGO_N7,
          CODIGOCONCATENADO LDESC_CATALOGO_N7,
          VALORPADRE NATID_CATALOGO_N6
     FROM REP_PRO_SIGLO.CAT_NIVELVALOR@SYG
    WHERE CLASIFICACION = 5 AND VALORPADRE IN (SELECT NATID_CATALOGO_N6 FROM MSTR_MAE_CATALOGO_N6)
/
