CREATE OR REPLACE FORCE VIEW MSTR_SIGLO.MSTR_MAE_CATALOGO_NA
(NATID_CATALOGO_NA, DESCR_CATALOGO_NA, LDESC_CATALOGO_NA, NATID_CATALOGO_N9)
AS 
SELECT ID NATID_CATALOGO_NA,
          DESCRIPCION DESCR_CATALOGO_NA,
          CODIGOCONCATENADO LDESC_CATALOGO_NA,
          VALORPADRE NATID_CATALOGO_N9
     FROM REP_PRO_SIGLO.CAT_NIVELVALOR@SYG
    WHERE CLASIFICACION = 5 AND VALORPADRE IN (SELECT NATID_CATALOGO_N9 FROM MSTR_MAE_CATALOGO_N9)
/
