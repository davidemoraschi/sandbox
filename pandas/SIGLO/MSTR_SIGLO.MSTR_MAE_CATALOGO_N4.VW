CREATE OR REPLACE FORCE VIEW MSTR_SIGLO.MSTR_MAE_CATALOGO_N4
(NATID_CATALOGO_N4, DESCR_CATALOGO_N4, LDESC_CATALOGO_N4, NATID_CATALOGO_N3)
AS 
SELECT ID NATID_CATALOGO_N4,
          DESCRIPCION DESCR_CATALOGO_N4,
          CODIGOCONCATENADO LDESC_CATALOGO_N4,
          VALORPADRE NATID_CATALOGO_N3
     FROM REP_PRO_SIGLO.CAT_NIVELVALOR@SYG
    WHERE CLASIFICACION = 5 AND VALORPADRE IN (SELECT NATID_CATALOGO_N3 FROM MSTR_MAE_CATALOGO_N3)
/
