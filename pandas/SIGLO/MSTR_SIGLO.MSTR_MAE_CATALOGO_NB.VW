CREATE OR REPLACE FORCE VIEW MSTR_SIGLO.MSTR_MAE_CATALOGO_NB
(NATID_CATALOGO_NB, DESCR_CATALOGO_NB, LDESC_CATALOGO_NB, NATID_CATALOGO_NA)
AS 
SELECT ID NATID_CATALOGO_NB,
          DESCRIPCION DESCR_CATALOGO_NB,
          CODIGOCONCATENADO LDESC_CATALOGO_NB,
          VALORPADRE NATID_CATALOGO_NA
     FROM REP_PRO_SIGLO.CAT_NIVELVALOR@SYG
    WHERE CLASIFICACION = 5 AND VALORPADRE IN (SELECT NATID_CATALOGO_NA FROM MSTR_MAE_CATALOGO_NA)
/
