CREATE OR REPLACE FORCE VIEW MSTR_SIGLO.MSTR_MAE_CATALOGO_N3
(NATID_CATALOGO_N3, DESCR_CATALOGO_N3, LDESC_CATALOGO_N3, NATID_CATALOGO_N2)
AS 
SELECT ID NATID_CATALOGO_N3,
          DESCRIPCION DESCR_CATALOGO_N3,
          CODIGOCONCATENADO LDESC_CATALOGO_N3,
          VALORPADRE NATID_CATALOGO_N2
     FROM REP_PRO_SIGLO.CAT_NIVELVALOR@SYG
    WHERE CLASIFICACION = 5 AND VALORPADRE IN (SELECT NATID_CATALOGO_N2 FROM MSTR_MAE_CATALOGO_N2)
/
