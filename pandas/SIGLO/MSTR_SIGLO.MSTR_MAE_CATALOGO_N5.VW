CREATE OR REPLACE FORCE VIEW MSTR_SIGLO.MSTR_MAE_CATALOGO_N5
(NATID_CATALOGO_N5, DESCR_CATALOGO_N5, LDESC_CATALOGO_N5, NATID_CATALOGO_N4)
AS 
SELECT ID NATID_CATALOGO_N5,
          DESCRIPCION DESCR_CATALOGO_N5,
          CODIGOCONCATENADO LDESC_CATALOGO_N5,
          VALORPADRE NATID_CATALOGO_N4
     FROM REP_PRO_SIGLO.CAT_NIVELVALOR@SYG
    WHERE CLASIFICACION = 5 AND VALORPADRE IN (SELECT NATID_CATALOGO_N4 FROM MSTR_MAE_CATALOGO_N4)
/
