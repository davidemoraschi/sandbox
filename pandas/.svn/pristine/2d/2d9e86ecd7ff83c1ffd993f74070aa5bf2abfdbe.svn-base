CREATE OR REPLACE FORCE VIEW MSTR_QUIROFANO.MSTR_MAE_PROCEDIMIENTOS
(PRC_CODIGO, PRC_GAR, GR_CODIGO, GG_CODIGO, PRC_GRUPO_CONC, 
 PRC_GRUPO_LE, PRC_NOMBRE, PRC_RDQ, PRC_TIEMPO_GAR)
AS 
SELECT PRC_CODIGO,
          PRC_GAR,
          PRC_GRUPO GR_CODIGO,
          GG_CODIGO,
          PRC_GRUPO_CONC,
          PRC_GRUPO_LE,
          PRC_NOMBRE,
          PRC_RDQ,
          --PRC_SITUACION,
          PRC_TIEMPO_GAR
     FROM REP_PRO_AGD.PROCEDIMIENTOS@DAE
     WHERE PRC_SITUACION = 0
/
