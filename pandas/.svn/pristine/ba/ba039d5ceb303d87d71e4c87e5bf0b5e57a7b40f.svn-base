CREATE OR REPLACE FUNCTION MSTR.FUNC_RANDOM_UF1
   RETURN VARCHAR2
AS
   v_DESCR_UNIDAD_FUNCIONAL1   MSTR_MAE_UNIDADES_FUNCIONALES1.DESCR_UNIDAD_FUNCIONAL1%TYPE;
BEGIN
     SELECT DESCR_UNIDAD_FUNCIONAL1
       INTO v_DESCR_UNIDAD_FUNCIONAL1
       FROM MSTR_MAE_UNIDADES_FUNCIONALES1
   --where ename like '%'
   ORDER BY DBMS_RANDOM.VALUE;

   RETURN v_DESCR_UNIDAD_FUNCIONAL1;
END;
/
